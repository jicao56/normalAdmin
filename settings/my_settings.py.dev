# -*- coding: utf-8 -*-
"""
自定义的配置
取自system数据库的t_config表
"""


# 自定义配置类
class MySettings(object):
    _instance = None

    def __new__(cls, *args, **kwargs):
        if cls._instance is None:
            cls._instance = super().__new__(cls)
        return cls._instance

    def __getattr__(self, item):
        return None


# 实例化自定义配置
settings_my = MySettings()

